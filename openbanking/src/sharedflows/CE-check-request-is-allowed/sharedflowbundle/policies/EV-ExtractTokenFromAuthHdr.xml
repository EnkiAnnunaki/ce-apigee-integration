<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables continueOnError="false" enabled="true" name="EV-ExtractTokenFromAuthHdr">
    <DisplayName>EV-ExtractTokenFromAuthHdr</DisplayName>
    <Header name="Authorization">
        <Pattern ignoreCase="false">Bearer {theAccessToken}</Pattern>
    </Header>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">request</Source>
</ExtractVariables>